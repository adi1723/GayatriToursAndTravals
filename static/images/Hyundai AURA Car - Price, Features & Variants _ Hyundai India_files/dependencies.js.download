function getServerURL(url) {
  var urlVal = url;
  if (location.port == '4502' || location.port == '4503') {
    urlVal = url + '.html';
  }
  if (location.port != '4502') {
    urlVal = urlVal.replace('/content/hyundai', '');
  }
  return urlVal;
}

