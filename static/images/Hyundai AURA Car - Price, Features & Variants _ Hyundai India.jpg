    var pathArrayVal = window.location.pathname.split('/');
    var tabTitleVal = pathArrayVal[5] ? pathArrayVal[5].split('.')[0] : '';

    var excludedPaths = [
        'click-to-buy',
        'contact-us',
        'investor-relations',
        'mobility-solutions',
        'hyundai-story',
        'connect-to-service'
    ];

    var currentPath = window.location.pathname;
    var isExcludedPage = excludedPaths.some(function(path) {
        return currentPath.includes(path);
    });

    var dataPush = {
        "event": "ga_event",
        "eventName": "click_event",
        "data": {
            "link": {
                "category": "click",
                "action": "explore_more_slideshow",
                "label": isNext
            }
        }
    };

    if (!isExcludedPage && tabTitleVal) {
        dataPush.data.link.page = "vehicle_tab_" + tabTitleVal;
    }

    // Push to dataLayer
    window.HMCdataLayer = window.HMCdataLayer || [];
    HMCdataLayer.push(dataPush);
});



